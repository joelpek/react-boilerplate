{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Basic React Boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-server && npm run build-client",
    "build-server": "babel src/server --out-dir ./dist",
    "build-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "node ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joelpek/react-boilerplate.git"
  },
  "keywords": [
    "Node"
  ],
  "author": "Joel Pekari",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joelpek/react-boilerplate/issues"
  },
  "homepage": "https://github.com/joelpek/react-boilerplate#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.0.0-beta.38",
    "babel-cli": "^6.26.0"
  }
}
